{
  "name": "tour",
  "version": "1.0.0",
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "react-native run-web",
    "example": "yarn --cwd example",
    "build:android": "cd andriod2/android && ./gradlew assembleDebug --no-daemon --console=plain -PreactNativeArchitectures=arm64-v8a",
    "build:ios": "cd example/ios && xcodebuild -workspace AwesomeModuleExample.xcworkspace -scheme AwesomeModuleExample -configuration Debug -sdk iphonesimulator CC=clang CPLUSPLUS=clang++ LD=clang LDPLUSPLUS=clang++ GCC_OPTIMIZATION_LEVEL=0 GCC_PRECOMPILE_PREFIX_HEADER=YES ASSETCATALOG_COMPILER_OPTIMIZATION=time DEBUG_INFORMATION_FORMAT=dwarf COMPILER_INDEX_STORE_ENABLE=NO",
    "bootstrap": "yarn example && yarn install && yarn example pods"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/material-bottom-tabs": "^6.2.15",
    "@react-navigation/native": "^6.1.8",
    "@react-navigation/native-stack": "^6.9.14",
    "@react-navigation/stack": "^6.3.16",
    "@rneui/themed": "^4.0.0-rc.8",
    "dotenv": "^16.0.3",
    "eas-cli": "^3.8.1",
    "react": "18.2.0",
    "react-native": "0.71.14",
    "react-native-confetti-cannon": "^1.5.2",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-gifted-chat": "^2.4.0",
    "react-native-maps": "^1.3.2",
    "react-native-maps-directions": "^1.9.0",
    "react-native-paper": "^4.9.2",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-vector-icons": "^9.2.0",
    "react-navigation-stack": "^2.10.4",
    "typescript": "^4.9.4",
    "react-native-pager-view": "6.1.2",
    "react-test-renderer": "^18.2.0"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.3.0",
    "jest": "^29.7.0",
    "react-test-renderer": "^18.2.0"
  },
  "private": true
}
